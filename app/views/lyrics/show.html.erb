


<h2><%= link_to @lyric.title, lyric_path( @lyric ) %></h2>
<%= form_tag get_lyric_version_path, remote: true do %>

<%= select_tag :lyric_version, options_for_select(@versions_for_select) %>

<small class="not-a-button">
	<%= submit_tag '[go]' %>
</small> 

<% end %>

<div id="lyric-version-container">

<%= render 'shared/lyric_version' %>

</div>